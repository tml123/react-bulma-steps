{
  "name": "react-bulma-steps",
  "version": "1.0.0",
  "description": "Stepper based on https://aramvisser.github.io/bulma-steps/ as a React Component",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config config/webpack.base.config.js --open --hot --history-api-fallback --env.PLATFORM=local --env.VERSION=stag",
    "prebuild": "webpack --mode production --config config/webpack.prod.config.js --env.PLATFORM=production --env.VERSION=stag --progress",
    "build": "node server",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "react",
    "bulma",
    "stepper",
    "steps"
  ],
  "author": "tml123",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.9.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "bulma-steps-component": "^0.5.3",
    "classnames": "^2.2.6",
    "d3": "^5.7.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0"
  }
}
